{
  "name": "@nexus/protocol",
  "version": "1.0.0",
  "description": "Core type definitions and schemas for NXML and NOG (Nexus Object Graph)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ast": {
      "import": "./dist/ast/index.mjs",
      "require": "./dist/ast/index.js",
      "types": "./dist/ast/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js",
      "types": "./dist/schemas/index.d.ts"
    },
    "./nog": {
      "import": "./dist/nog/index.mjs",
      "require": "./dist/nog/index.js",
      "types": "./dist/nog/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "nexus",
    "nxml",
    "protocol",
    "ast",
    "nog"
  ],
  "author": "Nexus Team",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
